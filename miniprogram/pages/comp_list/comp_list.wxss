.one_comp{
  /* border: 1px solid darkcyan; */
  padding: 40rpx 30rpx 40rpx 30rpx;
  display: flex;
  flex-direction: column;
  border-top: 1px solid rgb(224, 224, 224);
}

.pic{
  /* background-color: #ccc; */
  border-radius: 10px;
  width: 100%;
  display: flex;
  justify-content: center;
}

.pic image{
  width: 100%;
  display: block;
  border-radius: 10px;
}

.content{
  /* border: 1px solid blue; */
  margin-top: 10px;
  padding: 0 10px;
  display: flex;
  height: 120rpx;
  justify-content: space-between;
}

.left, .right{
  /* border: 1px solid red; */
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.right{
  text-align: right;
}

.name, .level{
  font-size: 38rpx;
}

.date, .number{
  font-size: 30rpx;
  color: rgb(150, 150, 150);
}